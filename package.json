{
  "name": "wept",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"bun dev:svelte\" \"bun dev:party\"",
    "dev:svelte": "vite dev --open",
    "dev:party": "partykit dev",
    "deploy:party": "partykit deploy",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "db:push": "drizzle-kit push:pg"
  },
  "dependencies": {
    "@lucia-auth/adapter-postgresql": "^2.0.1",
    "@lucia-auth/adapter-prisma": "^3.0.2",
    "@lucia-auth/oauth": "^3.3.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@tailwindcss/typography": "^0.5.10",
    "clsx": "^2.0.0",
    "daisyui": "^3.9.2",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "lucia": "^2.7.1",
    "lucide-svelte": "^0.284.0",
    "partysocket": "0.0.9",
    "postgres": "^3.3.5",
    "svelte": "^4.0.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.1",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "partykit": "^0.0.29",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte-check": "^3.4.3",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.11",
    "vitest": "^0.34.6"
  },
  "type": "module"
}
